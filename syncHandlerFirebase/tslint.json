{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "no-console": false,
      "no-bitwise": false,
      "object-literal-sort-keys": false,
      "no-var-requires": false,
      "ordered-imports": false,
      "max-classes-per-file": [true, 1, "exclude-class-expressions"]
    },
    "rulesDirectory": []
}
